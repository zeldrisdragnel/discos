<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>CompaniaServlet</servlet-name>
        <servlet-class>Controlador.CompaniaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CdServlet</servlet-name>
        <servlet-class>Controlador.CdServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ColeccionServlet</servlet-name>
        <servlet-class>Controlador.ColeccionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PistaServlet</servlet-name>
        <servlet-class>Controlador.PistaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SolistaServlet</servlet-name>
        <servlet-class>Controlador.SolistaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GrupoServlet</servlet-name>
        <servlet-class>Controlador.GrupoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CompaniaServlet</servlet-name>
        <url-pattern>/Compania</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CdServlet</servlet-name>
        <url-pattern>/Cd</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ColeccionServlet</servlet-name>
        <url-pattern>/Coleccion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PistaServlet</servlet-name>
        <url-pattern>/Pista</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SolistaServlet</servlet-name>
        <url-pattern>/Solista</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GrupoServlet</servlet-name>
        <url-pattern>/Grupo</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
